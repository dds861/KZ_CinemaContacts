<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dd.culture.places">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_cinema"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_cinema"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.dd.culture.places.activities.ActivityCityList">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dd.culture.places.activities.ActivityCityListDetails"
            android:parentActivityName="com.dd.culture.places.activities.ActivityCityList" />


    </application>

</manifest>